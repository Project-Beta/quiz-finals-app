*
	padding 0
	margin 0
	position relative
	box-sizing border-box
	text-rendering optimizeLegibility
	user-select none

*.flex
	display flex
	align-items center
	justify-content center

*.noselect
	user-select none

*.nodisplay
	display none

*.container
	width 100%
	height 100%

[contenteditable]:focus
	outline 0px solid transparent
