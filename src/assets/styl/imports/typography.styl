@font-face
	font-family "Code Light"
	src url("../../assets/fonts/code-light.woff") format("woff")
	font-weight 100
	font-size inherit
	line-height inherit
	font-style normal

@font-face
	font-family "Code Bold"
	src url("../../assets/fonts/code-bold.woff") format("woff")
